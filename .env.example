# Telegram bot token
BOT_TOKEN=123456:ABC-DEF...

# Admin group chat id (e.g. -1001234567890)
ADMIN_CHAT_ID=-1001234567890

# Public host where Telegram can call webhooks. Must be HTTPS and reachable.
WEBHOOK_HOST=https://your.domain

# Database url for asyncpg driver (note +asyncpg)
DATABASE_URL=postgresql+asyncpg://postgres:postgres@db:5432/supportdb
